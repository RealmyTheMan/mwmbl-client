#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/svelte-check@4.0.1_svelte@5.0.0-next.244_typescript@5.5.4/node_modules/svelte-check/bin/node_modules:/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/svelte-check@4.0.1_svelte@5.0.0-next.244_typescript@5.5.4/node_modules/svelte-check/node_modules:/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/svelte-check@4.0.1_svelte@5.0.0-next.244_typescript@5.5.4/node_modules:/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/svelte-check@4.0.1_svelte@5.0.0-next.244_typescript@5.5.4/node_modules/svelte-check/bin/node_modules:/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/svelte-check@4.0.1_svelte@5.0.0-next.244_typescript@5.5.4/node_modules/svelte-check/node_modules:/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/svelte-check@4.0.1_svelte@5.0.0-next.244_typescript@5.5.4/node_modules:/var/home/e/Zoding/mwmbl/mwmbl/sveltekit-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
